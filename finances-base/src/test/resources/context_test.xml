<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />

	<context:component-scan base-package="com.morcinek.finance" />

<!-- 			<ref bean="sortableTableModel" /> -->
	<bean id="parsers" class="java.util.ArrayList">
		<constructor-arg>
			<list value-type="java.lang.String">
				<value>Default</value>
				<value>Account</value>
				<value>Amount</value>
				<value>Currency</value>
				<value>Date</value>
				<value>Transaction</value>
			</list>
		</constructor-arg>
	</bean>
	<bean id="packageName" class="java.lang.String">
		<constructor-arg>
			<value>com.morcinek.finance.parse.objects</value>
		</constructor-arg>
	</bean>
	<bean id="fieldParser" class="com.morcinek.finance.parse.FieldParser">
		<property name="parsers">
			<ref bean="parsers" />
		</property>
	</bean>
	
		<!-- 	database beans -->
<!-- 	<bean id="dbHelper" class="com.morcinek.finance.database.DBHelper"> -->
<!-- 	</bean> -->
</beans>


